<?xml version="1.0" encoding="utf-8"?>
<Project InitialTargets="GenerateAppxContractFileTarget">
  <PropertyGroup>
    <ImplicitUsings>disable</ImplicitUsings>
    <Deterministic>true</Deterministic>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <Target Name="GenerateAppxContractFileTarget" BeforeTargets="Build" Condition="'$(UseAppxLibInfo)' == 'true' and '$(OutputType)' == 'Library'">
    <PropertyGroup>
      <_AppxContractFile>$(IntermediateOutputPath)AppxContract.g.cs</_AppxContractFile>
      <_AppxContractContent>
// &lt;auto-generated by GenerateAppxContractFileTarget (Directory.Build.props). /&gt;

// Copyright 2023 GZSkins, Inc. All rights reserved.
// Licensed under the Mozilla Public License, Version 2.0 (the "LICENSE.txt").
//
// This Source Code Form is subject to the terms of the Mozilla Public

// License, v. 2.0. If a copy of the MPL was not distributed with this
// file, You can obtain one at https://mozilla.org/MPL/2.0/.

[assembly: System.Runtime.CompilerServices.InternalsVisibleTo("GZSkinsX, ")]

namespace $(RootNamespace)
{
    internal static class AppxContract
    {
        
    }
}
      </_AppxContractContent>
    </PropertyGroup>
  
    <ItemGroup>
      <Compile Include="$(_AppxContractFile)">
        <Link>AppxContract.cs</Link>
        <Visible>false</Visible>
      </Compile>
    </ItemGroup>
  
    <WriteLinesToFile File="$(_AppxContractFile)" Lines="$(_AppxContractContent)" Overwrite="true" />
  </Target>

  <Target Name="CleanAppxContractFileTarget" BeforeTargets="IncrementalClean" Condition="Exists('$(_AppxContractFile)')" >
    <Delete Files="$(_AppxContractFile)" ContinueOnError="false" />
  </Target>
</Project>
